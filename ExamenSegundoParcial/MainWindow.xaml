<Window x:Class="ExamenSegundoParcial.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ExamenSegundoParcial"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="600">
    <StackPanel>
        <TextBlock FontSize="20">1.Información Personal>2.Contacto>3.Información de pago</TextBlock>
        <local:InformaciónPersonal x:Name="ControlInformaciónPersonal" Visibility="Visible"></local:InformaciónPersonal>
        <local:Contacto x:Name="ControlContacto" Visibility="Collapsed"></local:Contacto>
        <local:InfodePago x:Name="ControlInfodePago" Visibility="Collapsed"></local:InfodePago>
        <Button Width="60" HorizontalAlignment="Left" Margin="150,0,0,0" Name="Siguiente1" Click="Siguiente1_Click">Siguiente</Button>
    </StackPanel>
</Window>
